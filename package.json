      "axios": "^1.6.2",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "mssql": "^9.0.1"
    },
    "devDependencies": {
      "concurrently": "^8.2.0",
      "nodemon": "^3.0.1"
    }
  }
  